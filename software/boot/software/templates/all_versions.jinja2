{% extends "base.jinja2" %}

{% block title %}All Versions{% endblock %}

{% block content %}
<div class="container">
    <h1>{{ tool }} - All Versions</h1>
    <ul>
        {% for version in versions %}
        <li>{{ version }}</li>
        {% endfor %}
    </ul>
    <a href="{{ url_for('root') }}" class="btn btn-primary">Back</a>
</div>
{% endblock %}